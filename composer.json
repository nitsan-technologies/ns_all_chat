{
  "name": "nitsan/ns-all-chat",
  "type": "typo3-cms-extension",
  "description": "One of the only TYPO3 extension which provides to use most popular chat tool at your TYPO3 website.",
  "homepage": "https://extensions.typo3.org/extension/ns_all_chat/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "Chat",
    "ns_all_chat",
    "nitsan"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_all_chat/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsAllChat\\": "Classes/"
    }
  },
  "replace": {
      "ns_all_chat": "self.version",
      "nitsan/ns-all-chat": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_all_chat"
    }
  }
}
